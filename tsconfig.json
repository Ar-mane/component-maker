{
  "compilerOptions": {
    "outDir": "out",
    "lib": ["ES2022"],
    "module": "Node16",
    "target": "ES2022",
    "sourceMap": true,
    "rootDir": "src",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": ["node_modules", ".vscode-test", "web"]
}
