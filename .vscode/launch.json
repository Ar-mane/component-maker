{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dev Component maker",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--disable-extensions",
        "--skipGetOptimized",
        "--enable-proposed-api"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "preLaunchTask": "${defaultBuildTask}",
      "sourceMaps": true
    }
  ]
}
