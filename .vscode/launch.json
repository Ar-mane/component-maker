{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dev Component maker",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}", "--disable-extensions"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "preLaunchTask": "${defaultBuildTask}",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
